<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">

    <PreferenceCategory
        android:key="@string/prefs_currency"
        android:title="@string/name_currency">

        <ListPreference
            android:defaultValue="start"
            android:entries="@array/currencyKey"
            android:entryValues="@array/currencyValue"
            android:key="@string/preferncekey_current_currency"
            android:summary="%s"
            android:title="@string/prefs_currency_description" />
    </PreferenceCategory>

</PreferenceScreen>